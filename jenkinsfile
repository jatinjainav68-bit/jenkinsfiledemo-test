pipeline {
    agent any
    
    stages{
        stage ('stage1') {
            steps {
	          echo "hello this is first stage"
                 }
               }
        stage ('stage2') { 
            steps {
                 echo "hello this is second stage"
                 }
               }
        stage ('stage3') {
            steps {
                  echo "hello this is third stage"
                 }
           }
	  }
}
